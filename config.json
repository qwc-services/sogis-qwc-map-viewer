{
  "proxyUrl": "http://qwc2.sourcepole.ch/api/proxy?url=",
  "qwc2serverUrl": "http://qwc2.sourcepole.ch/api/",
  "translationsPath": "./translations",
  "assetsPath": "/assets",
  "plugins": {
      "mobile": [
        {
          "name": "Map",
          "cfg": {
            "projection": "EPSG:3857",
            "units": "m",
            "maxExtent": [
              -20037508.34, -20037508.34,
              20037508.34, 20037508.34
            ],
            "zoomControl": false,
            "mapType": "openlayers"
          }
        },
        {
          "name": "LocateButton"
        },
        {
          "name": "BackgroundSwitcher"
        },
        {
            "name": "TopBar",
            "cfg": {
              "menuItems": [
                {"key": "themes", "icon": "img/themes.svg"},
                {"key": "layers", "icon": "img/layers.svg"},
                {"key": "share", "icon": "img/share.svg"},
                {"key": "tools", "icon": "img/tools.svg", "subitems": [
                  {"key": "measure", "icon": "img/measure.svg"},
                  {"key": "draw", "icon": "img/draw.svg"},
                  {"key": "dxfexport", "icon": "img/dxfexport.svg"}
                ]},
                {"key": "print", "icon": "img/print.svg"},
                {"key": "help", "icon": "img/info.svg"}
              ],
              "search": {
                "minScale": 1000
              }
            }
        },
        {
          "name": "ThemeSwitcher"
        },
        {
          "name": "Measure"
        },
        {
          "name": "Identify",
          "cfg": {
            "params": {
              "FI_POINT_TOLERANCE": 32,
              "FI_LINE_TOLERANCE": 16,
              "FI_POLYGON_TOLERANCE": 8
            }
          }
        },
        {
          "name": "Share"
        },
        {
          "name": "Print"
        },
        {
          "name": "Help"
        },
        {
          "name": "MapCopyright"
        },
        {
          "name": "LayerTree"
        },
        {
          "name": "DxfExport"
        }
      ],
      "desktop": [
        {
          "name": "Map",
          "cfg": {
            "projection": "EPSG:3857",
            "units": "m",
            "maxExtent": [
              -20037508.34, -20037508.34,
              20037508.34, 20037508.34
            ],
            "zoomControl": false,
            "mapType": "openlayers"
          }
        },
        {
          "name": "LocateButton"
        },
        {
          "name": "ZoomIn"
        },
        {
          "name": "ZoomOut"
        },
        {
          "name": "BackgroundSwitcher"
        },
        {
            "name": "TopBar",
            "cfg": {
              "menuItems": [
                {"key": "themes", "icon": "img/themes.svg"},
                {"key": "layers", "icon": "img/layers.svg"},
                {"key": "share", "icon": "img/share.svg"},
                {"key": "tools", "icon": "img/tools.svg", "subitems": [
                  {"key": "measure", "icon": "img/measure.svg"},
                  {"key": "draw", "icon": "img/draw.svg"},
                  {"key": "dxfexport", "icon": "img/dxfexport.svg"}
                ]},
                {"key": "print", "icon": "img/print.svg"},
                {"key": "help", "icon": "img/info.svg"}
              ],
              "search": {
                "minScale": 1000
              }
            }
        },
        {
          "name": "BottomBar",
          "cfg": {
            "viewertitleUrl": "http://qgis.org/",
            "termsUrl":  "http://qgis.org/"
          }
        },
        {
          "name": "Measure"
        },
        {
          "name": "ThemeSwitcher"
        },
        {
          "name": "LayerTree"
        },
        {
          "name": "Identify",
          "cfg": {
            "params": {
              "FI_POINT_TOLERANCE": 16,
              "FI_LINE_TOLERANCE": 8,
              "FI_POLYGON_TOLERANCE": 4
            }
          }
        },
        {
          "name": "MapTip"
        },
        {
          "name": "Share"
        },
        {
          "name": "Print"
        },
        {
          "name": "Help"
        },
        {
          "name": "MapCopyright"
        },
        {
          "name": "DxfExport"
        }
      ]
  }
}
